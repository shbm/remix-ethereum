		"devdoc": {
			"details": "Implements staking functionality for NFTs with ERC20 token rewards This contract allows users to stake their NFTs and earn ERC20 tokens as rewards. Users can claim rewards every second (configurable) and withdraw their NFTs at any time. The contract ensures only the original staker can claim rewards and withdraw the NFT.",
			"errors": {
				"OwnableInvalidOwner(address)": [
					{
						"details": "The owner is not a valid owner account. (eg. `address(0)`)"
					}
				],
				"OwnableUnauthorizedAccount(address)": [
					{
						"details": "The caller account is not authorized to perform an operation."
					}
				]
			},
			"kind": "dev",
			"methods": {
				"calculateRewards(uint256)": {
					"details": "Calculates pending rewards for a staked NFT",
					"params": {
						"tokenId": "ID of the NFT to calculate rewards for"
					},
					"returns": {
						"_0": "uint256 Amount of reward tokens earned"
					}
				},
				"claimRewards(uint256)": {
					"details": "Claims accumulated rewards for a staked NFT",
					"params": {
						"tokenId": "ID of the NFT to claim rewards for"
					}
				},
				"constructor": {
					"details": "Constructor initializes the staking contract with token addresses",
					"params": {
						"_nftToken": "Address of the NFT contract that can be staked",
						"_rewardToken": "Address of the ERC20 reward token contract"
					}
				},
				"onERC721Received(address,address,uint256,bytes)": {
					"details": "Handles the receipt of an NFT",
					"params": {
						"from": "Address of the NFT sender",
						"tokenId": "ID of the transferred NFT"
					},
					"returns": {
						"_0": "bytes4 The function selector to confirm receipt"
					}
				},
				"owner()": {
					"details": "Returns the address of the current owner."
				},
				"renounceOwnership()": {
					"details": "Leaves the contract without owner. It will not be possible to call `onlyOwner` functions. Can only be called by the current owner. NOTE: Renouncing ownership will leave the contract without an owner, thereby disabling any functionality that is only available to the owner."
				},
				"transferOwnership(address)": {
					"details": "Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner."
				},
				"withdrawNFT(uint256)": {
					"details": "Withdraws a staked NFT and claims any remaining rewards",
					"params": {
						"tokenId": "ID of the NFT to withdraw"
					}
				}
			},
			"title": "NFT Staking Contract",
			"version": 1
		},
		"userdoc": {
			"events": {
